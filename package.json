{
  "name": "ts-misc",
  "author": "anthony",
  "version": "1.5.3",
  "description": "Typescript Miscellaneous Library",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "transpile": "tsc -p tsconfig.json",
    "beautify": "js-beautify -r --editorconfig ./dist/**/*.{js,ts,tsx}",
    "build": "yarn transpile && yarn beautify",
    "preinstall": "yarn build && yarn rm-r \"./src\""
  },
  "dependencies": {
    "js-beautify": "^1.13.5",
    "prettier": "^2.2.1",
    "rm-r": "^1.0.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "resolutions": {}
}
